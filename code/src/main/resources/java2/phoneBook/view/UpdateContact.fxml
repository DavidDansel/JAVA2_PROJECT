<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane minHeight="440.0" minWidth="715.0" prefHeight="440.0" prefWidth="715.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="java2.phoneBook.view.UpdateContactController">
    <children>
        <TableView fx:id="contactsListsTable" layoutX="7.0" layoutY="30.0" prefHeight="380.0" prefWidth="696.0">
            <columns>
                <TableColumn fx:id="id" prefWidth="42.0" text="ID" />
                <TableColumn fx:id="lName" prefWidth="103.0" text="L. NAME" />
                <TableColumn fx:id="fName" prefWidth="78.0" text="F. NAME" />
                <TableColumn fx:id="nickName" prefWidth="112.0" text="N. NAME" />
                <TableColumn fx:id="phoneNo" prefWidth="96.0" text="PHONE NO" />
                <TableColumn fx:id="address" prefWidth="90.0" text="ADDRESS" />
                <TableColumn fx:id="email" minWidth="0.0" prefWidth="87.0" text="EMAIL" />
                <TableColumn fx:id="bDate" minWidth="0.0" prefWidth="105.0" text="B.DATE" />
            </columns>
        </TableView>
      <Button fx:id="updateRowBtn" layoutX="281.0" layoutY="410.0" mnemonicParsing="false" onAction="#updateSelectedRow" text="UPDATE SELECTED ROW" />
      <Button fx:id="homeBtn" layoutX="8.0" layoutY="1.0" mnemonicParsing="false" onAction="#handleHomeButton" text="HOME" />
    </children>
</AnchorPane>
